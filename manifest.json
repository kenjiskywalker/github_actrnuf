{
  "manifest_version": 2,
  "name": "GitHub Add color to recently not updated file",
  "version": "0.2",
  "description": "Add color to recently not updated file on GitHub.",
  "content_scripts": [
    {
      "matches": ["http://*.github.com/*", "https://*.github.com/*"],
      "js": ["jquery-1.11.1.js", "content.js"]
    }
  ],
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png"
  }
}


